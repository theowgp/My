function res = GV(x, a)

N = length(x);
res = zeros(N, 1);


%My version

% temp = 0;
% for i = 1:N
%     temp = temp + a(i)*x(i);
% end
% 
% res = 2*temp*a;


%Marcos version

temp = 0;
for i = 1:N
    for j = 1:N
        res(i) = 
        temp = temp + (x(i) - x(j))^2;
    end
end

res = 4 * res;



end

